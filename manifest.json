{
  "manifest_version": 3,
  "name": "Trusted Domains Collector",
  "version": "1.1",
  "description": "Собирает все сторонние домены, загружаемые на странице",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Trusted Domains",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
